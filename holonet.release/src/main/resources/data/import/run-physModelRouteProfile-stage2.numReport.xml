<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:hn="https://github.com/akraievoy/holonet/runner"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
           https://github.com/akraievoy/holonet/runner
           https://github.com/akraievoy/holonet/raw/master/ext/holonet.runner-1.3.1.xsd"
       default-init-method="start"
       default-destroy-method="stop">
  <hn:Experiment
      id="physModelRoutes-1-report" depends=""
      description="Routing profiling [stage2] Numeric report">
    <hn:Config name="default" description="default">
    </hn:Config>
  </hn:Experiment>

  <bean class="org.akraievoy.cnet.export.domain.export.ExportNumeric" id="main">
    <property name="systemParameters"
              value="main.systemParameters,main.skippedParameters,main.axisParameters,main.keys,main.targetFile,main.targetDir,main.hiddenParameters"/>
    <!-- DYNAMIC -->
    <property name="hiddenParameters" value="connPreference.alpha,connPreference.beta"/>
    <!-- DYNAMIC -->
    <property name="skippedParameters" value=""/>
    <!-- DYNAMIC -->
    <property name="axisParameters" value="locationMetric.nodes,structure.baseDegree"/>
    <!-- DYNAMIC -->
    <property name="keys" value="ref_duration_j,ref_duration_fw"/>

    <property name="targetDir" value="data/reports/routeProfile"/>
    <property name="targetFile" value="timing.csv"/>
  </bean>
</beans>