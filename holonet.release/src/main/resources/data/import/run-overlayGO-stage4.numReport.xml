<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:hn="https://github.com/akraievoy/holonet/runner"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
           https://github.com/akraievoy/holonet/runner
           https://github.com/akraievoy/holonet/raw/master/ext/holonet.runner-1.3.1.xsd"
       default-init-method="start"
       default-destroy-method="stop">
  <hn:Experiment
      id="overlayGO-4-numReport" depends=""
      description="Overlay GO [stage4] Numeric export">
    <hn:Config name="default" description="template, don't use that">
      <hn:Param
          name="main.hiddenParameters"
          value="ovlReq.psi,ovlReq.phi,esOvl.seed,ovlReqThresh.minToMaxRatio,ovlReq.sigma,ovlReqThresh.minAbsValue,onf.nu,onf.omega,structure.baseDegree,entropySourceLocGen.seed,entropySource.seed,entropySourceLocation.seed,locationMetric.nodes"/>
      <hn:Param name="main.axisParameters" value="ovlGenOpt.generation"/>
      <hn:Param name="main.skippedParameters" value="ovlGenOpt.specimenIndex"/>
    </hn:Config>
    <hn:Config name="timing" description="Timing">
      <hn:Param name="main.keys" value="main_duration_millis,main_duration_text"/>
      <hn:Param name="main.targetFile" value="go_timing.csv"/>
    </hn:Config>
    <hn:Config name="getState" description="Genetics Autotuning State">
      <hn:Param name="main.keys"
                value="state.similarityMean,state.fitnessDev,state.mutate,state.crossover,state.minElemFitness"/>
      <hn:Param name="main.targetFile" value="go_genState.csv"/>
    </hn:Config>
    <hn:Config name="fitness" description="Fitness">
      <hn:Param name="main.keys" value="specimen.fitness"/>
      <hn:Param name="main.axisParameters" value="ovlGenOpt.generation,ovlGenOpt.specimenIndex"/>
      <hn:Param name="main.skippedParameters" value=""/>
      <hn:Param name="main.targetFile" value="go_fitness.csv"/>
    </hn:Config>
    <hn:Config name="composites" description="Mutators/Breeders/Conditions Pass/Use Rates">
      <hn:Param name="main.keys"
                value="mutators.0,mutators.1,mutators.success.0,mutators.success.1,mutators.uses.0,mutators.uses.1,breeders.0,breeders.1,breeders.success.0,breeders.success.1,breeders.uses.0,breeders.uses.1"/>
      <hn:Param name="main.skippedParameters" value="ovlGenOpt.specimenIndex"/>
      <hn:Param name="main.targetFile" value="go_composites.csv"/>
    </hn:Config>
  </hn:Experiment>

  <bean class="org.akraievoy.cnet.export.domain.export.ExportNumeric" id="main">
    <property name="systemParameters"
              value="main.systemParameters,main.skippedParameters,main.axisParameters,main.keys,main.targetFile,main.targetDir,main.hiddenParameters"/>
    <!-- DYNAMIC -->
    <property name="hiddenParameters" value=""/>
    <!-- DYNAMIC -->
    <property name="skippedParameters" value=""/>
    <!-- DYNAMIC -->
    <property name="axisParameters" value=""/>
    <!-- DYNAMIC -->
    <property name="keys" value=""/>
    <!-- DYNAMIC -->
    <property name="targetDir" value="data/reports/go"/>
    <!-- DYNAMIC -->
    <property name="targetFile" value="unnamed.txt"/>
  </bean>
</beans>