<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd"
       default-init-method="start"
       default-destroy-method="stop">
  <bean id="batch" class="org.akraievoy.base.runner.domain.BatchRunner$Batch">
    <constructor-arg>
      <list>
        <bean class="org.akraievoy.base.runner.domain.BatchRunner$ComponentExperiment">
          <constructor-arg value="run-overlayGO-stage1.physDataset"/>
          <constructor-arg value="big-1k"/>
        </bean>
        <bean class="org.akraievoy.base.runner.domain.BatchRunner$ComponentExperiment">
          <constructor-arg value="run-overlayGO-stage2.ovlDataset"/>
          <constructor-arg value="nu20"/>
          <constructor-arg><list><value>run-overlayGO-stage1.physDataset</value></list></constructor-arg>
        </bean>
        <bean class="org.akraievoy.base.runner.domain.BatchRunner$ComponentExperiment">
          <constructor-arg value="run-overlayGO-stage3.genetics"/>
          <constructor-arg value="minEff19x50x256-tune"/>
          <constructor-arg><list><value>run-overlayGO-stage2.ovlDataset</value></list></constructor-arg>
        </bean>
      </list>
    </constructor-arg>
  </bean>
</beans>
