<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:hn="https://github.com/akraievoy/holonet/runner/1.3.3"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
           https://github.com/akraievoy/holonet/runner/1.3.3
           https://github.com/akraievoy/holonet/raw/master/ext/holonet.runner-1.3.3.xsd"
       default-init-method="start"
       default-destroy-method="stop">
  <bean id="batch" class="org.akraievoy.base.runner.domain.BatchRunner$Batch">
    <constructor-arg>
      <list>
        <bean class="org.akraievoy.base.runner.domain.BatchRunner$ComponentExperiment">
          <constructor-arg value="run-overlayGO-stage1.physDataset"/>
          <constructor-arg value="big-1k"/>
        </bean>
        <bean class="org.akraievoy.base.runner.domain.BatchRunner$ComponentExperiment">
          <constructor-arg value="run-overlayGO-stage2.ovlDataset"/>
          <constructor-arg value="nu20"/>
          <constructor-arg><list><value>run-overlayGO-stage1.physDataset</value></list></constructor-arg>
        </bean>
        <bean class="org.akraievoy.base.runner.domain.BatchRunner$ComponentExperiment">
          <constructor-arg value="run-overlayGO-stage3.genetics"/>
          <constructor-arg value="minEff17"/>
<!--
          <constructor-arg value="minEff21x50x256"/>
-->
          <constructor-arg><list><value>run-overlayGO-stage2.ovlDataset</value></list></constructor-arg>
        </bean>
        <bean class="org.akraievoy.base.runner.domain.BatchRunner$ComponentExperiment">
          <constructor-arg value="run-p2p-stage0.seed"/>
          <constructor-arg value="3x7"/>
        </bean>
        <bean class="org.akraievoy.base.runner.domain.BatchRunner$ComponentExperiment">
          <constructor-arg value="run-p2p-stage1.protoSet"/>
          <constructor-arg value="default"/>
        </bean>
        <bean class="org.akraievoy.base.runner.domain.BatchRunner$ComponentExperiment">
          <constructor-arg value="run-p2p-stage2.paramSpace"/>
          <constructor-arg value="extraLoops"/>
        </bean>
        <bean class="org.akraievoy.base.runner.domain.BatchRunner$ComponentExperiment">
          <constructor-arg value="run-p2p-stage3.attackDestab"/>
          <constructor-arg value="default"/>
          <constructor-arg><list>
              <value>run-overlayGO-stage3.genetics</value>
              <value>run-p2p-stage0.seed</value>
              <value>run-p2p-stage1.protoSet</value>
              <value>run-p2p-stage2.paramSpace</value>
          </list></constructor-arg>
        </bean>
        <bean class="org.akraievoy.base.runner.domain.BatchRunner$ComponentExperiment">
          <constructor-arg value="run-p2p-stage3.preDestab"/>
          <constructor-arg value="default"/>
          <constructor-arg><list>
              <value>run-overlayGO-stage3.genetics</value>
              <value>run-p2p-stage0.seed</value>
              <value>run-p2p-stage1.protoSet</value>
              <value>run-p2p-stage2.paramSpace</value>
          </list></constructor-arg>
        </bean>
        <bean class="org.akraievoy.base.runner.domain.BatchRunner$ComponentExperiment">
          <constructor-arg value="run-p2p-stage3.static"/>
          <constructor-arg value="default"/>
          <constructor-arg><list>
              <value>run-overlayGO-stage3.genetics</value>
              <value>run-p2p-stage0.seed</value>
              <value>run-p2p-stage1.protoSet</value>
              <value>run-p2p-stage2.paramSpace</value>
          </list></constructor-arg>
        </bean>
        <bean class="org.akraievoy.base.runner.domain.BatchRunner$ComponentExperiment">
          <constructor-arg value="run-p2p-stage3.staticDestab"/>
          <constructor-arg value="default"/>
          <constructor-arg><list>
              <value>run-overlayGO-stage3.genetics</value>
              <value>run-p2p-stage0.seed</value>
              <value>run-p2p-stage1.protoSet</value>
              <value>run-p2p-stage2.paramSpace</value>
          </list></constructor-arg>
        </bean>
      </list>
    </constructor-arg>
  </bean>
</beans>
