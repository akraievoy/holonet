<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd"
       default-init-method="start"
       default-destroy-method="stop">
  <bean id="batch" class="org.akraievoy.base.runner.domain.BatchRunner$Batch">
    <constructor-arg>
      <list>
        <bean class="org.akraievoy.base.runner.domain.BatchRunner$ComponentExperiment">
          <constructor-arg value="run-p2p-stage0.seed"/>
          <constructor-arg value="3x7"/>
        </bean>
        <bean class="org.akraievoy.base.runner.domain.BatchRunner$ComponentExperiment">
          <constructor-arg value="run-p2p-stage1.protoSet"/>
          <constructor-arg value="default"/>
        </bean>
        <bean class="org.akraievoy.base.runner.domain.BatchRunner$ComponentExperiment">
          <constructor-arg value="run-p2p-stage2.paramSpace"/>
          <constructor-arg value="failProf"/>
        </bean>
        <bean class="org.akraievoy.base.runner.domain.BatchRunner$ComponentExperiment">
          <constructor-arg value="run-p2p-stage3.attackDestab"/>
          <constructor-arg value="default"/>
          <constructor-arg >
            <list>
              <value>run-p2p-stage0.seed</value>
              <value>run-p2p-stage1.protoSet</value>
              <value>run-p2p-stage2.paramSpace</value>
            </list>
          </constructor-arg>
        </bean>
        <bean class="org.akraievoy.base.runner.domain.BatchRunner$ComponentExperiment">
          <constructor-arg value="run-p2p-stage3.preDestab"/>
          <constructor-arg value="default"/>
          <constructor-arg >
            <list>
              <value>run-p2p-stage0.seed</value>
              <value>run-p2p-stage1.protoSet</value>
              <value>run-p2p-stage2.paramSpace</value>
            </list>
          </constructor-arg>
        </bean>
        <bean class="org.akraievoy.base.runner.domain.BatchRunner$ComponentExperiment">
          <constructor-arg value="run-p2p-stage3.static"/>
          <constructor-arg value="default"/>
          <constructor-arg >
            <list>
              <value>run-p2p-stage0.seed</value>
              <value>run-p2p-stage1.protoSet</value>
              <value>run-p2p-stage2.paramSpace</value>
            </list>
          </constructor-arg>
        </bean>
        <bean class="org.akraievoy.base.runner.domain.BatchRunner$ComponentExperiment">
          <constructor-arg value="run-p2p-stage3.staticDestab"/>
          <constructor-arg value="default"/>
          <constructor-arg >
            <list>
              <value>run-p2p-stage0.seed</value>
              <value>run-p2p-stage1.protoSet</value>
              <value>run-p2p-stage2.paramSpace</value>
            </list>
          </constructor-arg>
        </bean>
      </list>
    </constructor-arg>
  </bean>
</beans>
